@model IEnumerable<User>

@{
    ViewBag.Title = "Accounts";
}

<h2>@ViewBag.Title</h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var account in Model)
        {
            <tr>
                <td>@account.Id</td>
                <td>@account.UserName</td>
                <td>@account.Email</td>
            </tr>
        }
    </tbody>
</table>
